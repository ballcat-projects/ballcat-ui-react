{
  "name": "ballcat",
  "version": "5.0.0-beta.3",
  "private": true,
  "description": "Ballcat",
  "scripts": {
    "start": "cross-env NODE_OPTIONS=--openssl-legacy-provider PORT=9002 HOST=0.0.0.0 REACT_APP_ENV=start UMI_ENV=dev umi dev",
    "start:uat": "cross-env NODE_OPTIONS=--openssl-legacy-provider PORT=9002 HOST=0.0.0.0 REACT_APP_ENV=start UMI_ENV=uat umi dev",
    "start:prod": "cross-env NODE_OPTIONS=--openssl-legacy-provider PORT=9002 HOST=0.0.0.0 REACT_APP_ENV=start UMI_ENV=prod umi dev",
    "build": "cross-env UMI_ENV=dev umi build",
    "build:uat": "cross-env UMI_ENV=uat umi build",
    "build:prod": "cross-env UMI_ENV=prod umi build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "deploy": "npm run site && npm run gh-pages",
    "dev": "npm run start",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier --check \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "precommit": "lint-staged",
    "prettier": "prettier -c --write \"src/**/*\"",
    "tsc": "tsc --noEmit"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "@ant-design/icons": "~4.5.0",
    "@ant-design/pro-descriptions": "~1.9.21",
    "@ant-design/pro-form": "~1.43.3",
    "@ant-design/pro-layout": "~6.26.0",
    "@ant-design/pro-table": "~2.56.3",
    "@ant-design/pro-utils": "~1.43.2",
    "@umijs/route-utils": "~2.0.3",
    "antd": "~4.17.1",
    "classnames": "~2.2.6",
    "cropperjs": "~1.5.12",
    "crypto-js": "~4.0.0",
    "lodash": "~4.17.11",
    "moment": "~2.25.3",
    "omit.js": "~2.0.2",
    "rc-util": "~5.16.0",
    "react": "~17.0.0",
    "react-activation": "~0.9.0",
    "react-color": "~2.19.3",
    "react-copy-to-clipboard": "~5.0.4",
    "react-cropper": "~2.1.8",
    "react-dom": "~17.0.0",
    "react-helmet-async": "~1.0.4",
    "react-transition-group": "~4.4.2",
    "umi": "~v3.5.0",
    "umi-serve": "~1.9.10",
    "wangeditor": "~4.7.8"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "~2.0.2",
    "@types/classnames": "~2.2.7",
    "@types/crypto-js": "~4.0.1",
    "@types/express": "~4.17.0",
    "@types/google.maps": "~3.47.1",
    "@types/history": "~4.7.2",
    "@types/jest": "~26.0.0",
    "@types/lodash": "~4.14.144",
    "@types/react": "~17.0.0",
    "@types/react-color": "~3.0.4",
    "@types/react-dom": "~17.0.0",
    "@types/react-helmet": "~6.1.0",
    "@umijs/fabric": "~2.6.2",
    "@umijs/plugin-blocks": "~2.0.5",
    "@umijs/plugin-esbuild": "~1.0.1",
    "@umijs/preset-ant-design-pro": "~1.2.0",
    "@umijs/preset-react": "~1.7.4",
    "@umijs/yorkie": "~2.0.3",
    "carlo": "~0.9.46",
    "cross-env": "~7.0.0",
    "cross-port-killer": "~1.1.1",
    "detect-installer": "~1.0.1",
    "enzyme": "~3.11.0",
    "eslint": "~7.32.0",
    "express": "~4.17.1",
    "gh-pages": "~3.0.0",
    "jsdom-global": "~3.0.2",
    "lint-staged": "~10.0.0",
    "prettier": "~2.0.1",
    "puppeteer-core": "~8.0.0",
    "stylelint": "~13.0.0",
    "typescript": "~4.2.2",
    "webpack-sources": "~3.2.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "gitHooks": {
    "commit-msg": "fabric verify-commit"
  }
}
